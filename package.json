{
  "name": "hf-element-ui",
  "version": "1.2.0",
  "description": "Heartflow Element UI",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "predeploy": "npm run build:theme && npm run build",
    "deploy": "gh-pages -d dist",
    "dev": "concurrently \"npm run serve\" \"et --watch\"",
    "clean:theme": "rm -rf shared/lib",
    "build:theme": "npm run clean:theme && et"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HeartFlow/hf-element-ui.git"
  },
  "author": "HeartFlow FrontEnd Team <frontend@heartflow.com>",
  "bugs": {
    "url": "https://github.com/HeartFlow/hf-element-ui/issues"
  },
  "homepage": "https://github.com/HeartFlow/hf-element-ui",
  "dependencies": {
    "element-ui": "2.3.6",
    "lodash.debounce": "^4.0.8",
    "vue": "^2.6.10",
    "vue-router": "^3.0.6",
    "vue-visible": "^1.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.0",
    "concurrently": "^4.1.1",
    "element-theme": "github:HeartFlow/element-theme",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "gh-pages": "^2.0.1",
    "vue-template-compiler": "^2.6.10"
  },
  "element-theme": {
    "config": "./element-variables.css",
    "theme": "../theme",
    "out": "./shared/lib",
    "watchGlobs": [
      "./element-variables.css",
      "./theme/**/*.scss"
    ],
    "components": [
      "autocomplete",
      "breadcrumb",
      "breadcrumb-item",
      "button",
      "checkbox",
      "checkbox-group",
      "col",
      "collapse",
      "collapse-item",
      "date-picker",
      "dialog",
      "dropdown",
      "dropdown-item",
      "dropdown-menu",
      "form",
      "form-item",
      "input",
      "input-number",
      "message",
      "notification",
      "option",
      "option-group",
      "popover",
      "radio",
      "radio-button",
      "radio-group",
      "row",
      "select",
      "spinner",
      "switch",
      "table",
      "table-column",
      "tooltip",
      "transfer"
    ]
  }
}
